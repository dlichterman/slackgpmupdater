{
  "name": "Slack Google Play Music Updater",
  "version": "0.1",
  "description": "Update Slack with google play music!",
  "permissions": ["storage","tabs","webNavigation","https://play.google.com/music/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}